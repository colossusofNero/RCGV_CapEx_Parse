{
  "name": "TipTap",
  "displayName": "TipTap",
  "expo": {
    "name": "TipTap",
    "slug": "tiptap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2563EB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tiptap.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "TipTap needs camera access to scan QR codes",
        "NSFaceIDUsageDescription": "TipTap uses Face ID to secure your payments",
        "NFCReaderUsageDescription": "TipTap uses NFC for contactless payments"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2563EB"
      },
      "package": "com.tiptap.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "NFC",
        "USE_FINGERPRINT",
        "USE_BIOMETRIC"
      ]
    },
    "scheme": "tiptap",
    "extra": {
      "eas": {
        "projectId": "8e74187b-a6bf-4a10-ad3c-831bb2d5e513"
      }
    },
    "owner": "colossusonner0"
  }
}
