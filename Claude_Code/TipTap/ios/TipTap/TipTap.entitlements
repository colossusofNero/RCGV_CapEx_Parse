<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- NFC Tag Reading Capability -->
	<key>com.apple.developer.nfc.readersession.formats</key>
	<array>
		<string>NDEF</string>
		<string>TAG</string>
	</array>

	<!-- Push Notifications -->
	<key>aps-environment</key>
	<string>development</string>

	<!-- Associated Domains for Universal Links -->
	<key>com.apple.developer.associated-domains</key>
	<array>
		<string>applinks:tiptap.com</string>
		<string>applinks:*.tiptap.com</string>
		<string>applinks:app.tiptap.com</string>
		<string>applinks:api.tiptap.com</string>
	</array>

	<!-- Apple Pay Merchant IDs -->
	<key>com.apple.developer.in-app-payments</key>
	<array>
		<string>merchant.com.tiptap.app</string>
		<string>merchant.com.tiptap.app.staging</string>
	</array>

	<!-- App Groups for Widget Extension and Data Sharing -->
	<key>com.apple.security.application-groups</key>
	<array>
		<string>group.com.tiptap.app</string>
		<string>group.com.tiptap.app.widgets</string>
		<string>group.com.tiptap.app.notifications</string>
	</array>

	<!-- Keychain Access Groups -->
	<key>keychain-access-groups</key>
	<array>
		<string>$(AppIdentifierPrefix)com.tiptap.app</string>
		<string>$(AppIdentifierPrefix)group.com.tiptap.app</string>
	</array>

	<!-- Background Modes -->
	<key>com.apple.developer.background-modes</key>
	<array>
		<string>background-processing</string>
		<string>background-fetch</string>
		<string>remote-notification</string>
	</array>

	<!-- Network Extensions (for VPN or proxy if needed) -->
	<key>com.apple.developer.networking.networkextension</key>
	<array>
		<string>packet-tunnel-provider</string>
		<string>app-proxy-provider</string>
	</array>

	<!-- Inter-App Audio (for payment sound notifications) -->
	<key>inter-app-audio</key>
	<true/>

	<!-- HealthKit (if integrating with health-related payments) -->
	<key>com.apple.developer.healthkit</key>
	<true/>
	<key>com.apple.developer.healthkit.access</key>
	<array>
		<!-- Add specific health data types if needed -->
	</array>

	<!-- Siri Integration -->
	<key>com.apple.developer.siri</key>
	<true/>

	<!-- ClassKit (for educational institution payments) -->
	<key>com.apple.developer.ClassKit-environment</key>
	<string>development</string>

	<!-- Data Protection Class -->
	<key>com.apple.developer.default-data-protection</key>
	<string>NSFileProtectionComplete</string>

	<!-- Family Controls (for parental control features) -->
	<key>com.apple.developer.family-controls</key>
	<true/>

	<!-- Screen Time API -->
	<key>com.apple.developer.screen-time</key>
	<true/>

	<!-- Communication Notifications (for payment alerts) -->
	<key>com.apple.developer.usernotifications.communication</key>
	<true/>

	<!-- Time Sensitive Notifications -->
	<key>com.apple.developer.usernotifications.time-sensitive</key>
	<true/>

	<!-- Critical Alerts (for important payment notifications) -->
	<key>com.apple.developer.usernotifications.critical-alerts</key>
	<true/>

	<!-- Wireless Accessory Configuration -->
	<key>com.apple.external-accessory.wireless-configuration</key>
	<true/>

	<!-- Home Automation (for IoT payment devices) -->
	<key>com.apple.developer.homekit</key>
	<true/>

	<!-- AutoFill Credential Provider -->
	<key>com.apple.developer.authentication-services.autofill-credential-provider</key>
	<true/>

	<!-- Shared Web Credentials -->
	<key>com.apple.developer.associated-domains</key>
	<array>
		<string>webcredentials:tiptap.com</string>
		<string>webcredentials:app.tiptap.com</string>
	</array>

	<!-- Sign in with Apple -->
	<key>com.apple.developer.applesignin</key>
	<array>
		<string>Default</string>
	</array>

	<!-- App Clip (for lightweight payment experiences) -->
	<key>com.apple.developer.parent-application-identifiers</key>
	<array>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	</array>

	<!-- Personal VPN -->
	<key>com.apple.developer.networking.vpn.api</key>
	<array>
		<string>allow-vpn</string>
	</array>

	<!-- Extended Virtual Addressing -->
	<key>com.apple.developer.kernel.extended-virtual-addressing</key>
	<true/>

	<!-- Maps -->
	<key>com.apple.developer.maps</key>
	<true/>

	<!-- Increased Memory Limit -->
	<key>com.apple.developer.kernel.increased-memory-limit</key>
	<true/>
</dict>
</plist>